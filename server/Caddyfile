corgi.wiki {
	redir https://www.corgi.wiki
}

www.corgi.wiki {
	reverse_proxy localhost:8001
}

staging.corgi.wiki {
	@robotsTxt {
		path /robots.txt
	}
	handle @robotsTxt {
		respond "User-agent: *\nDisallow: /" 200
	}
	reverse_proxy localhost:8018
}

