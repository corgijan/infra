git.corgijan.dev {
        rate_limit {
                distributed
                zone static_example {
                        match {
                                method GET
                        }
                        key static
                        events 50
                        window 3s
                }
                zone dynamic_example {
                        key {remote_host}
                        events 50
                        window 3s
                }
                log_key
        }
        handle_path /.within.website/x/cmd/anubis/static/img/happy.webp {
                rewrite * /happy.webp
                root * /srv
                file_server
        }
        handle_path /.within.website/x/cmd/anubis/static/img/pensive.webp {
                rewrite * /happy.webp
                root * /srv
                file_server
        }
        handle_path /happy.webp {
                rewrite * /happy.webp
                root * /srv
                file_server
        }
        @robotsTxt {
                path /robots.txt
        }
        reverse_proxy http://maine:31000
}

